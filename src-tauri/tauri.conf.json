{
  "$schema": "../node_modules/@tauri-apps/cli/config.schema.json",
  "productName": "Mihoyo Sticker",
  "version": "1.1.0",
  "identifier": "me.clovu.mihoyo-sticker",
  "build": {
    "frontendDist": "../out",
    "devUrl": "http://localhost:3000",
    "beforeDevCommand": "pnpm dev",
    "beforeBuildCommand": "pnpm build"
  },
  "app": {
    "windows": [
      {
        "fullscreen": false,
        "height": 430,
        "width": 666,
        "resizable": false,
        "decorations": false,
        "center": true,
        "visible": false
      }
    ],
    "security": {
      "csp": null
    },
    "withGlobalTauri": true,
    "macOSPrivateApi": true
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}
